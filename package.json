{
  "name": "idor-chat",
  "version": "0.0.1",
  "description": "An insecure chat web application.",
  "devDependencies": {
    "ts-node": "3.3.0",
    "@types/node": "^8.0.29",
    "typescript": "^3.7.5"
  },
  "dependencies": {
    "bcrypt": "^4.0.0",
    "body-parser": "^1.19.0",
    "class-validator": "^0.11.0",
    "concurrently": "^5.1.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "mysql": "^2.14.1",
    "path": "^0.12.7",
    "reflect-metadata": "^0.1.10",
    "typeorm": "0.2.22"
  },
  "scripts": {
    "start": "node build/index.js",
    "start:dev": "concurrently --kill-others \"npm run nodemon:watch\" \"npm run ngServe\"",
    "nodemon:watch": "nodemon -e ts --exec \"npm run tsc\"",
    "ngServe": "cd frontend && ./node_modules/@angular/cli/bin/ng build && ./node_modules/@angular/cli/bin/ng serve",
    "postinstall": "cd frontend && npm install",
    "build": "rm -rf build && tsc && cd frontend && rm -rf dist && ./node_modules/@angular/cli/bin/ng build --aot --prod"
  },
  "keywords": [
    "web security",
    "web application security",
    "webappsec",
    "owasp",
    "pentest",
    "pentesting",
    "security",
    "vulnerable",
    "vulnerability"
  ],
  "nodemonConfig": {
    "ignore": [
      "frontend"
    ]
  }
}
